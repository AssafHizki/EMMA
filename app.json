{
  "expo": {
    "name": "EMMA",
    "slug": "emma",
    "description": "EMMA App",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.14",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "notification": {
      "icon": "./assets/images/icon-notification.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.emma.app",
      "buildNumber": "0.0.14",
      "supportsTablet": true,
      "requireFullScreen": false
    },
    "android": {
      "package": "com.emma.app",
      "versionCode": 14,
      "permissions": []
    }
  }
}
